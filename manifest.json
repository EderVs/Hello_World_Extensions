{
    "manifest_version": 2,
	"name": "Hello World!",
    "version": "1.0",
    "description": "An extension that only appears a hello world when you click",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_title": "Hey!"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    }
}